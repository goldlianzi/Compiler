const m=7, n=85;
var x,y,z,q,r;
// this is a note
procedure multiply;
    var a,b;
    begin /* this is a note */
        a:=x; b:=y; z:=0;
        while b>0 do
            begin
                if odd b then z:=z+a;
                a:=2*a; b:=b/2;
            end;
            write(a, b)
    end;
begin
    x:=m; y:=n;
    call multiply;
end.
